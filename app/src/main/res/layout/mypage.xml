<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="295dp"
            android:layout_height="66dp"
            android:layout_marginStart="50dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="100dp"
            android:layout_marginEnd="50dp"
            android:layout_marginRight="50dp"
            android:background="#ff565c"
            android:gravity="center"
            android:text="   MY PAGE   "
            android:textColor="@color/textColor"
            android:textSize="45dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <!--<Button-->
        <!--android:id="@+id/btn_custom_login"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:text="카카오 로그인"
         tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="255dp"-->
        <!--tools:layout_editor_absoluteX="0dp"/>-->
        <com.kakao.usermgmt.LoginButton
            android:id="@+id/btn_kakao_login"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView12"
            tools:ignore="MissingConstraints" />


        <Button
            android:id="@+id/basket_list"
            android:layout_width="282dp"
            android:layout_height="57dp"
            android:layout_marginStart="50dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="50dp"
            android:layout_marginRight="50dp"
            android:background="#ff656c"
            android:gravity="center"
            android:text="@string/basket_list"
            android:textColor="@color/textColor"
            android:textSize="35sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView17"
            tools:ignore="HardcodedText,OnClick" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="295dp"
            android:layout_height="108dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/kakao"
            android:gravity="center|right"
            android:text="카카오톡 문의    "
            android:textSize="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_kakao_login"
            tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded,SpUsage" />

    </android.support.constraint.ConstraintLayout>
</FrameLayout>